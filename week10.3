n = int(input())
arr = list(map(int, input().split()))

peaks = []

if n == 1:
    peaks.append(arr[0])
elif arr[0] >= arr[1]:
    peaks.append(arr[0])

for i in range(1, n - 1):
    if arr[i] >= arr[i - 1] and arr[i] >= arr[i + 1]:
        peaks.append(arr[i])

if arr[n - 1] >= arr[n - 2]:
    peaks.append(arr[n - 1])

# Output
print(*peaks)
