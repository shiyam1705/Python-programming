def zip_lists(list1, list2):
    zipped_list = []
    for sublist1, sublist2 in zip(list1, list2):
        zipped_list.append(sublist1 + sublist2)
    return zipped_list

# Taking input for row size
m = int(input())

# Taking input for column size
n = int(input())

# Taking input for list1
list1 = []
for _ in range(m):
    row = []
    for _ in range(n):
        row.append(int(input()))
    list1.append(row)

# Taking input for list2
list2 = []
for _ in range(m):
    row = []
    for _ in range(n):
        row.append(int(input()))
    list2.append(row)

# Zipping the lists
zipped_list = zip_lists(list1, list2)

# Outputting the zipped list
print(zipped_list)
