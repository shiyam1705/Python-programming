def count_characters(s):
    letters =0
    digits=0
    special_symbols=0
    
    for char in s:
        if char.isdigit():
            digits+=1
        elif char.isalpha():
            letters +=1
        else:
            special_symbols+=1
    
    print(letters)
    print(digits)
    print(special_symbols)
    
s=input().strip()

count_characters(s)
