def factor_and_return_nth(n, p):
    factors = []
    for i in range(1, int(n**0.5) + 1):
        if n % i == 0:
            factors.append(i)
            if i != n // i:
                factors.append(n // i)
    factors.sort()
    return factors[p - 1] if p <= len(factors) else 0

# Taking input
n = int(input())
p = int(input())

# Output
print(factor_and_return_nth(n, p))
